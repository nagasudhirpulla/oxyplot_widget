<Window x:Class="Dashboard.Widgets.Oxyplot.LinePlotConfigEditWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Dashboard.Widgets.Oxyplot"
        mc:Ignorable="d"
        Title="LinePlotConfigEditWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        
        
        <StackPanel Orientation="Horizontal" Grid.Row="2">
            <Button Content="OK" Click="OkBtnClick" Padding="5"></Button>
            <Button Content="Cancel" Click="CancelBtnClick" Padding="5"></Button>
        </StackPanel>
    </Grid>
</Window>
